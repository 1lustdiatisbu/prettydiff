    "devDependencies": {
        "5082"  : "^1.0.0",
        "jslint": "^0.9.0"
    },
    "homepage"       : "http://prettydiff.com/",
    "keywords"       : [
        "diff",
        "prettydiff",
        "pretty diff",
        "pretty print",
        "pretty-print",
        "beautify",
        "minify",
        "xml",
        "html",
        "css",
        "jsx",
        "es6",
        "javascript"
    ],
    "license"        : "SEE LICENSE IN license.txt",
    "main"           : "prettydiff.js",
    "maintainers"    : [
        {
            "email": "info@prettydiff.com",
            "name" : "austincheney"
        }
    ],
    "name"           : "prettydiff",
    "repository"     : {
        "type": "git",
        "url" : "https://github.com/prettydiff/prettydiff"
    },
    "scripts"        : {
        "prepublish": "node test/lint.js",
        "publish"   : "node node_modules/5082/5082.js",
        "test"      : "node test/lint.js"
    },
    "version"        : "1.16.34"
