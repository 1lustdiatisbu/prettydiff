    "bin"            : "./bin/prettydiff",
    "bugs"           : {
        "email": "info@prettydiff.com",
        "url"  : "https://github.com/prettydiff/prettydiff/issues"
    },
    "config5082"     : {
        "beautify": true,
        "files"   : [
            ".bower.json", "bower.json", "prettydiff.js"
